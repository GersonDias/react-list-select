{
  "name": "react-list-select",
  "version": "0.3.0",
  "description": "List with selectable and navigable items",
  "keywords": [
    "react-component"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "keys.js",
    "list.js",
    "list-item.js",
    "README.md",
    "CHANGELOG.md"
  ],
  "author": "Navaru",
  "contributors": [
    "Hawken Rives <hawkrives@gmail.com>"
  ],
  "license": "ISC",
  "repository": "hawkrives/react-list-select",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-react": "^6.24.1",
    "babelify": "^5.0.3",
    "browser-sync": "^2.2.1",
    "browserify": "^9.0.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "exorcist": "^0.1.6",
    "gh-pages-deploy": "^0.3.0",
    "prettier": "^1.11.1",
    "react": ">=15",
    "watchify": "^2.4.0"
  },
  "babel": {
    "presets": [
      "react"
    ]
  },
  "dependencies": {
    "classnames": "^1.1.4",
    "lodash": "^3.3.1"
  },
  "config": {
    "browserify": "--debug -t [babelify] -e index.js"
  },
  "scripts": {
    "lint": "eslint --cache *.js example/*.js",
    "make-example": "browserify $npm_package_config_browserify -e example/index.js > example/bundle.js",
    "build": "babel ./*.js --out-dir ./dist",
    "start": "npm run browser-sync & npm run watch; wait",
    "prepublishOnly": "npm run build",
    "deploy": "gh-pages-deploy",
    "post-deploy": "npm run clean"
  },
  "gh-pages-deploy": {
    "staticpath": "example",
    "prep": [
      "make-example"
    ],
    "noprompt": true
  }
}
