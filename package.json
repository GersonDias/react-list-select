{
  "name": "react-list-select",
  "version": "0.3.0",
  "description": "List with selectable and navigable items",
  "keywords": [
    "react-component"
  ],
  "main": "index.js",
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md"
  ],
  "author": "Navaru",
  "contributors": [
    "Hawken Rives <hawkrives@gmail.com>"
  ],
  "license": "ISC",
  "repository": "hawkrives/react-list-select",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browserify": "^16.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.68.0",
    "gh-pages-deploy": "^0.4.2",
    "prettier": "^1.11.1",
    "react": ">=15"
  },
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": true,
    "bracketSpacing": false,
    "trailingComma": "all"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^3.3.1"
  },
  "scripts": {
    "build": "babel ./*.js --out-dir ./dist",
    "deploy": "gh-pages-deploy",
    "flow": "flow",
    "lint": "eslint --cache *.js example/*.js",
    "example": "npm run build && browserify -e example/index.js > example/bundle.js",
    "prepublishOnly": "npm run build",
    "prettify": "prettier --write *.js example/*.{css,js}",
    "post-deploy": "npm run clean"
  },
  "gh-pages-deploy": {
    "staticpath": "example",
    "prep": [
      "example"
    ],
    "noprompt": true
  }
}
